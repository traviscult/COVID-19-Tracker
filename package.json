{
  "name": "covid-tracker",
  "version": "1.0.0",
  "description": "A Corona Virus informational application that allows users to view current data and utilize a symptom checker",
  "main": "./server.js",
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^4.6.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "mapbox-gl": "^1.12.0",
    "mongoose": "^5.9.24",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-usa-map": "^1.5.0"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node ./server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/traviscult/project-3"
  },
  "author": "S Sharma, Joy Chen, Travis Cultreri, Cate Dunn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/traviscult/project-3/issues"
  },
  "homepage": "https://covid19-tracker-applicaiton.herokuapp.com/"
}
